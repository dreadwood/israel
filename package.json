{
  "name": "israil",
  "scripts": {
    "editorconfig": "editorconfig-cli",
    "stylelint": "stylelint \"source/sass/**/*.scss\" --syntax scss",
    "test": "npm run editorconfig && npm run stylelint && eslint source/js/",
    "build": "gulp build",
    "start": "gulp start",
    "webp": "gulp webp",
    "sprite": "gulp sprite",
    "gh-pages": "push-dir --dir=dist --branch=gh-pages"
  },
  "browserslist": [
    "last 4 versions",
    "IE 11"
  ],
  "editorconfig-cli": [
    "*.json",
    "*.js",
    "source/*.html",
    "source/js/**/*.js",
    "source/img/**/*.svg",
    "source/sass/**/*.{sass,scss}"
  ],
  "devDependencies": {
    "@htmlacademy/editorconfig-cli": "1.0.0",
    "autoprefixer": "9.5.1",
    "browser-sync": "2.27.10",
    "del": "4.1.1",
    "eslint": "4.16.0",
    "eslint-config-htmlacademy": "0.4.2",
    "gulp": "4.0.2",
    "gulp-csso": "3.0.1",
    "gulp-imagemin": "5.0.3",
    "gulp-plumber": "1.2.1",
    "gulp-postcss": "8.0.0",
    "gulp-posthtml": "3.0.4",
    "gulp-rename": "1.4.0",
    "gulp-sass": "5.1.0",
    "gulp-sourcemaps": "2.6.5",
    "gulp-svgstore": "7.0.1",
    "gulp-webp": "4.0.1",
    "posthtml-include": "1.2.0",
    "push-dir": "0.4.1",
    "sass": "1.55.0",
    "stylelint": "14.13.0",
    "stylelint-config-htmlacademy": "0.1.12"
  }
}
